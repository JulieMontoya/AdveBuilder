INSERT INTO rooms VALUES(1,'you are at the West end of the street where it joins a main road, outside number 1.',1);
INSERT INTO rooms VALUES(2,'you are in a cluttered room, absolutely stuffed with paintings of cats, photographs of cats, cartoon drawings of cats and ornaments shaped like cats in pottery, glass and plastic.',1);
INSERT INTO rooms VALUES(3,'you are outside number 2.',1);
INSERT INTO rooms VALUES(4,'you are inside number 2.',1);
INSERT INTO rooms VALUES(5,'you are inside a bedroom which clearly belongs to a child with an active imagination and a temperament to match.',1);
INSERT INTO rooms VALUES(6,'you are outside a small newsagent''s shop. the shop is closed. an alley leads to the North.',1);
INSERT INTO rooms VALUES(7,'you are in an alleyway beside the newsagent''s shop. a set of metal steps leads up to a door.',1);
INSERT INTO rooms VALUES(8,'you are at the top of a metal staircase. there is a door to the East with the number 3A on it.',1);
INSERT INTO rooms VALUES(9,'you are inside a dingy flat over the newsagent''s shop. the furniture consists of a few wooden crates and a threadbare mattress. the smell of abject despair pervades the place.',1);
INSERT INTO rooms VALUES(10,'you are outside number 4.',1);
INSERT INTO rooms VALUES(11,'you are in a dimly-lit room with Eastern-influenced wall hangings, old rock concert posters, record sleeves and fractal patterns. wind chimes are tinkling and there is a heavy scent of joss sticks in the air.',1);
INSERT INTO rooms VALUES(12,'you are at the East end of the street outside a large and imposing library building. a set of stone steps leads up to the entrance proper.',1);
